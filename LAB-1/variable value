// Complete the main method by displaying the values of the variables a and b.
// Sample Input 0

// 10 20
// Sample Output 0

// Value of a is 10 and value of b is 20

import java.util.*;
import java.io.*;
public class main{
  public static void main(String[] args){
    Scanner scn = new Scanner (System.in);
    int a = scn.nextInt();
    int b = scn.nextInt();
    
    System.out.println("Value of a is " + a +" and " +"value of b is " +b);
}

}

                                                   