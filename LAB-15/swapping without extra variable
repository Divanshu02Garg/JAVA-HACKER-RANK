// Given two integers, 'a' and 'b' swap their value without using extra variable.

// Input Format

// Two space separated integers

// Constraints

// None

// Output Format

// Two space separated integers

// Sample Input 0

// 10 30
// Sample Output 0

// 30 10
// Sample Input 1

// 406 720
// Sample Output 1

// 720 406

import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
         Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int b=sc.nextInt();
        a=a+b;
        b=a-b;
        a=a-b;
        System.out.println(a+" "+b);
    }
}




